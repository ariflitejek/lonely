{
  "name": "yt2dl",
  "version": "1.0.7",
  "description": "YouTube Audio & Video Downloader Using NodeJS",
  "private": true,
  "author": "ipincamp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipincamp/yt2dl/issues"
  },
  "homepage": "https://github.com/ipincamp/yt2dl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipincamp/yt2dl.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=18.x",
    "npm": ">=9.x"
  },
  "scripts": {
    "build": "tsc",
    "check:format": "prettier --check .",
    "check:eslint": "eslint . --ext js --ext ts --ext tsx",
    "check:types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "start": "npm run build && node dist/index.js",
    "test": "nodemon"
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-validation": "^4.1.0",
    "ffmpeg-static": "^5.1.0",
    "sanitize-filename": "^1.6.3",
    "ytdl-core": "^4.11.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "eslint": "^8.42.0",
    "eslint-config-standard-with-typescript": "^35.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.8",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^5.1.3"
  },
  "keywords": [
    "javascript",
    "js",
    "mp3downloader",
    "node-ytdl-core",
    "node-ytdl",
    "nodejs",
    "typescript",
    "y2mate",
    "youtube-dl",
    "youtube-downloader",
    "youtube-music",
    "youtubedl",
    "youtubeplaylist",
    "yt-converter",
    "yt-downloader",
    "yt-mp3",
    "yt-music",
    "yt2mp3",
    "ytdl-core",
    "ytdl-nodejs",
    "ytdl",
    "ytdownload",
    "ytdownloader",
    "ytplaylist"
  ]

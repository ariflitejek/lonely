{
  "name": "yt2dl",
  "version": "1.0.6",
  "description": "YouTube Audio & Video Downloader Using NodeJS",
  "private": true,
  "author": "ipincamp",
  "license": "GPL-3.0",
  "bugs": {
    "url": "https://github.com/ipincamp/yt2dl/issues"
  },
  "homepage": "https://github.com/ipincamp/yt2dl#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ipincamp/yt2dl.git"
  },
  "main": "index.js",
  "engines": {
    "node": ">=18",
    "npm": ">=8"
  },
  "scripts": {
    "build": "tsc",
    "check-format": "prettier --check .",
    "check-lint": "eslint . --ext ts --ext tsx --ext js",
    "check-types": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "prepare": "husky install",
    "start": "node index.js",
    "test": "npx nodemon"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npx pretty-quick --staged ng lint ng test",
      "pre-push": "ng build --aot true"
    }
  },
  "dependencies": {
    "express": "^4.18.2",
    "express-validation": "^4.1.0",
    "ffmpeg-static": "^5.1.0",
    "lodash": "^4.17.21",
    "sanitize-filename": "^1.6.3",
    "sqlite3": "^5.1.4",
    "ytdl-core": "^4.11.3"
  },
  "devDependencies": {
    "@types/express": "^4.17.16",
    "@types/lodash": "^4.14.191",
    "@typescript-eslint/eslint-plugin": "^5.50.0",
    "eslint": "^8.33.0",
    "eslint-config-standard-with-typescript": "^34.0.0",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.6.1",
    "eslint-plugin-promise": "^6.1.1",
    "husky": "^8.0.3",
    "nodemon": "^2.0.20",
    "prettier": "^2.8.3",
    "pretty-quick": "^3.1.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  },
  "keywords": [
    "javascript",
    "js",
    "mp3downloader",
    "node-ytdl-core",
    "node-ytdl",
    "nodejs",
    "typescript",
    "y2mate",
    "youtube-dl",
    "youtube-downloader",
    "youtube-music",
    "youtubedl",
    "youtubeplaylist",
    "yt-converter",
    "yt-downloader",
    "yt-mp3",
    "yt-music",
    "yt2mp3",
    "ytdl-core",
    "ytdl-nodejs",
    "ytdl",
    "ytdownload",
    "ytdownloader",
    "ytplaylist"
  ]
}

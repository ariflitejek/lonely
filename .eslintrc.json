{
  "env": {
    "browser": true,
    "es2021": true,
    "node": true
  },
  "extends": ["airbnb-base"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "consistent-return": "off",
    "indent": ["error", 2],
    "linebreak-style": ["error", "windows"],
    "no-return-assign": "off",
    "radix": "off"
  }
}
